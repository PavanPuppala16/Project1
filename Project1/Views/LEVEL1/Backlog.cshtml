@model IEnumerable<Project1.Models.GraduationP>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SHORT LIST STUDENTS</title>
</head>
<body>
    <meta name="viewport" content="width=device-width" />


    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="https://www.markuptag.com/bootstrap/5/css/bootstrap.min.css">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
    <div class="container" style="border" color="blue">
    </div>

   

<h4>level1 selected students</h4>
<div class="container">
   SELECT FOR LEVEL1
</div>

<table class="table table-striped">
    <tr>
            <th>HTDID</th>
            <th>EngineeringCollegeName</th>
            <th>Branch </th>
            <th>EngineeringPassout</th>
            <th>Backlog</th>
            <th>GraduationAggregate</th>
            <th>ACCEPT</th>
            <th>REJECT</th>
            
    </tr>
    @foreach (var item in Model)
    {
        <tr>
                <td>@item.HTDID</td>
                <td>@item.EngineeringCollegeName</td>
                <td>@item.Branch</td>
                <td>@item.EngineeringPassout</td>
                <td>@item.Backlog</td>
                <td>@item.GraduationAggregate</td>
                <td>
                    @Html.ActionLink("ACCEPT","EDIT","LEVEL1", new {Sno = item.HTDID},new{onclick="return confirm('are you selected to Level2')"}) 
                    
                </td>
                <td>@Html.ActionLink("REJECT","DELETEDATA","LEVEL1", new { rollno= item.HTDID},new{onclick="return confirm('sorry.your rejected')"}))</td>
        </tr>
    }

</table>
</body>
</html>
